MODULE    = winebus.sys
IMPORTS   = ntoskrnl advapi32
EXTRALIBS = $(IOKIT_LIBS) $(UDEV_LIBS) $(PTHREAD_LIBS) -Wl,--subsystem,unixlib
EXTRAINCL = $(UDEV_CFLAGS) $(SDL2_CFLAGS)

EXTRADLLFLAGS = -Wl,--subsystem,native

C_SRCS = \
	bus_iohid.c \
	bus_sdl.c \
	bus_udev.c \
	hid.c \
	main.c \
	unixlib.c

RC_SRCS = winebus.rc
